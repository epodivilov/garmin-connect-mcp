{
  "name": "garmin-connect-mcp",
  "version": "0.1.0",
  "description": "MCP server for Garmin Connect integration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "garmin-connect-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "garmin",
    "connect",
    "model-context-protocol",
    "garmin-connect",
    "fitness",
    "health",
    "training",
    "claude",
    "ai"
  ],
  "author": "Evgenii Podivilov <evgeniy.podivilov@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epodivilov/garmin-connect-mcp.git"
  },
  "homepage": "https://github.com/epodivilov/garmin-connect-mcp#readme",
  "bugs": {
    "url": "https://github.com/epodivilov/garmin-connect-mcp/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "garmin-connect": "^1.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.16.1"
}